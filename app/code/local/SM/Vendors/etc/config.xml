                </customer_create_account_vendor_email_approved_template>

                <customer_create_account_notice_admin_about_signup_template translate="label" module="customer">
                    <label>For Vendor: Notice Admin When New Vendor Signup</label>
                    <file>smvendors/notice_admin_about_new_signup.html</file>
                    <type>html</type>
                </customer_create_account_notice_admin_about_signup_template>
            </email>
        </template>		
    </global>

    <frontend>
        <layout>
            <updates>
                <smvendors>
                    <file>sm/vendors.xml</file>
                </smvendors>
                <smvendors_sales>
                    <file>sm/vendors_sales.xml</file>
                </smvendors_sales>                
            </updates>
        </layout>
        <routers>
            <smvendors>
                <use>standard</use>
                <args>
                    <module>SM_Vendors</module>
                    <frontName>vendor</frontName>
                </args>
            </smvendors>
        </routers>
        <events>
            <checkout_cart_product_add_after>
                <observers>
                    <smvendors>
                        <type>singleton</type>
                        <class>smvendors/observer</class>
                        <method>hookCartProductAddAfter</method>
                    </smvendors>
                </observers>
            </checkout_cart_product_add_after>

            <controller_action_predispatch_checkout_cart_index>
                <observers>
                    <smvendors>
                        <type>singleton</type>
                        <class>smvendors/observer</class>
                        <method>hookCartProductAddAfter</method>
                    </smvendors>
                </observers>
            </controller_action_predispatch_checkout_cart_index>

            <checkout_cart_update_items_after>
                <observers>
                    <smvendors>
                        <type>singleton</type>
                        <class>smvendors/observer</class>
                        <method>hookCartProductUpdateItemAfter</method>
                    </smvendors>
                </observers>
            </checkout_cart_update_items_after>

            <catalog_product_get_final_price>
                <observers>
                    <smvendors>
                        <class>smvendors/price_observer</class>
                        <method>processFrontFinalPrice</method>
                    </smvendors>
                </observers>
            </catalog_product_get_final_price>
        </events>
    </frontend>

    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <smvendors before="Mage_Adminhtml">SM_Vendors_Adminhtml</smvendors>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>

    <adminhtml>
        <layout>
            <updates>
                <smvendors>
                    <file>sm/vendors.xml</file>
                </smvendors>
                <smvendors_sales>
                    <file>sm/vendors_sales.xml</file>
                </smvendors_sales>
            </updates>
        </layout>
        <events>
            <adminhtml_customer_prepare_save>
                <observers>
                    <smvendors>
                        <type>singleton</type>
                        <class>smvendors/observer</class>
                        <method>hookAdminCustomerPrepareSave</method>
                    </smvendors>
                </observers>
            </adminhtml_customer_prepare_save>

            <customer_group_save_before>
                <observers>
                    <smvendors>
                        <type>singleton</type>
                        <class>smvendors/observer</class>
                        <method>hookCustomerGroupBeforeSave</method>
                    </smvendors>
                </observers>
            </customer_group_save_before>

            <sales_quote_add_item>
                <observers>
                    <smvendors>
                        <type>singleton</type>
                        <class>smvendors/observer</class>
                        <method>hookCartProductAddAfter</method>
                    </smvendors>
                </observers>
            </sales_quote_add_item>

            <catalog_product_get_final_price>
                <observers>
                    <smvendors>
                        <class>smvendors/price_observer</class>
                        <method>processAdminFinalPrice</method>
                    </smvendors>
                </observers>
            </catalog_product_get_final_price>

            <admin_system_config_changed_section_smvendors>
                <observers>
                    <sm_core_init_service>
                        <type>singleton</type>
                        <class>smcore/observer</class>
                        <method>refreshStatus</method>
                    </sm_core_init_service>
                </observers>
            </admin_system_config_changed_section_smvendors>

        </events> 
    </adminhtml>

    <default>
        <smvendors>
            <general>
