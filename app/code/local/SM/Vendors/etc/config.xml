        <resources>
            <smvendors_setup>
                <setup>
                    <module>SM_Vendors</module>
                    <class>Mage_Catalog_Model_Resource_Eav_Mysql4_Setup</class>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </smvendors_setup>
            <smvendors_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </smvendors_write>
            <smvendors_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </smvendors_read>
        </resources>
		
        <models>
            <smvendors>
                <class>SM_Vendors_Model</class>
                <resourceModel>smvendors_mysql4</resourceModel>
            </smvendors>
            <smvendors_mysql4>
                <class>SM_Vendors_Model_Mysql4</class>
                <entities>
                    <vendor>
                        <table>sm_vendor</table>
                    </vendor>
                    <order>
                        <table>sm_vendor_order</table>
                    </order>
                    <page>
                        <table>sm_vendor_page</table>
                    </page>
                    <banner>
                        <table>sm_vendor_banner</table>
                    </banner>
                    <vendor_customer>
                        <table>sm_vendor_customer</table>
                    </vendor_customer>
                </entities>
            </smvendors_mysql4>
            <customer>
                <rewrite>
                    <customer>SM_Vendors_Model_Override_Customer</customer>
                </rewrite>
            </customer>
            <customer_resource>
                <rewrite>
                    <group_collection>SM_Vendors_Model_Override_Customer_Resource_Group_Collection</group_collection>
                </rewrite>
            </customer_resource>
            <sales>
                <rewrite>
                    <service_order>SM_Vendors_Model_Override_Sales_Service_Order</service_order>
                    <order_pdf_invoice>SM_Vendors_Model_Override_Sales_Order_Pdf_Invoice</order_pdf_invoice>
                    <order_pdf_shipment>SM_Vendors_Model_Override_Sales_Order_Pdf_Shipment</order_pdf_shipment>
                    <order_pdf_shipment_packaging>SM_Vendors_Model_Override_Sales_Order_Pdf_Shipment_Packaging</order_pdf_shipment_packaging>
                    <order_pdf_creditmemo>SM_Vendors_Model_Override_Sales_Order_Pdf_Creditmemo</order_pdf_creditmemo>
					
                    <order_invoice>SM_Vendors_Model_Override_Sales_Order_Invoice</order_invoice>
                    <order_shipment>SM_Vendors_Model_Override_Sales_Order_Shipment</order_shipment>
                    <order_creditmemo>SM_Vendors_Model_Override_Sales_Order_Creditmemo</order_creditmemo>
                </rewrite>
            </sales>
			<!-- 
			<checkout>
			<rewrite>
			<type_onepage>SM_Vendors_Model_Override_Checkout_Type_Onepage</type_onepage>
			</rewrite>
			</checkout>
			-->

            <salesrule>
                <rewrite>
                    <rule_condition_combine>SM_Vendors_Model_Override_Salesrule_Rule_Condition_Combine</rule_condition_combine> 
                    <rule_condition_product_combine>SM_Vendors_Model_Override_Salesrule_Rule_Condition_Product_Combine</rule_condition_product_combine>
                </rewrite>
            </salesrule>
            <catalogrule>
                <rewrite>
                    <rule_condition_combine>SM_Vendors_Model_Override_CatalogRule_Rule_Condition_Combine</rule_condition_combine>
				<!-- 			        <observer>SM_Vendors_Model_Override_CatalogRule_Observer</observer> -->
                </rewrite>
            </catalogrule>
            <catalog>
                <rewrite>
                    <layer_filter_item>SM_Vendors_Model_Override_Catalog_Layer_Filter_Item</layer_filter_item>
                </rewrite>
            </catalog>
            <eav>
                <rewrite>
                    <entity_type>SM_Vendors_Model_Override_Eav_Entity_Type</entity_type>
                </rewrite>
            </eav>
            <!-- override for customer report  -->
            <reports_resource>
                <rewrite>
                    <customer_totals_collection>SM_Vendors_Model_Override_Reports_Resource_Totals_Collection</customer_totals_collection>
                </rewrite>
            </reports_resource>


           <sales_resource>
                <rewrite>
                    <!--<report_bestsellers_collection>SM_Vendors_Model_Override_Sales_Mysql4_Report_Bestsellers_Collection</report_bestsellers_collection>-->
                    <!--<report_bestsellers>SM_Vendors_Model_Override_Sales_Mysql4_Report_Bestsellers</report_bestsellers>-->
                    <report_refunded_collection_order>SM_Vendors_Model_Override_Sales_Resource_Report_Refunded_Collection_Order</report_refunded_collection_order>
                    <report_refunded_collection_refunded>SM_Vendors_Model_Override_Sales_Resource_Report_Refunded_Collection_Refunded</report_refunded_collection_refunded>
                    <report_refunded>SM_Vendors_Model_Override_Sales_Resource_Report_Refunded</report_refunded>
                    <report_invoiced>SM_Vendors_Model_Override_Sales_Resource_Report_Invoiced</report_invoiced>
                    <report_invoiced_collection_order>SM_Vendors_Model_Override_Sales_Resource_Report_Invoiced_Collection_Order</report_invoiced_collection_order>
                    <report_invoiced_collection_invoiced>SM_Vendors_Model_Override_Sales_Resource_Report_Invoiced_Collection_Invoiced</report_invoiced_collection_invoiced>
                    <report_shipping>SM_Vendors_Model_Override_Sales_Mysql4_Report_Shipping</report_shipping>
                </rewrite>
            </sales_resource>

            <tax_resource>
                <rewrite>
                    <report_collection>SM_Vendors_Model_Override_Tax_Resource_Report_Collection</report_collection>
                    <report_updatedat_collection>SM_Vendors_Model_Override_Tax_Resource_Report_Updatedat_Collection</report_updatedat_collection>
                    <report_tax_createdat>SM_Vendors_Model_Override_Tax_Resource_Report_Tax_Createdat</report_tax_createdat>
                    <report_tax_updatedat>SM_Vendors_Model_Override_Tax_Resource_Report_Tax_Updatedat</report_tax_updatedat>
                </rewrite>
            </tax_resource>

        </models>

        <helpers>
            <smvendors>
                <class>SM_Vendors_Helper</class>
            </smvendors>
            <adminhtml>
                <rewrite>
					<!-- orverride dashboard -->
                    <dashboard_order>SM_Vendors_Helper_Override_Adminhtml_Dashboard_Order</dashboard_order>
					<!-- orverride dashboard -->
                </rewrite>
            </adminhtml>
        </helpers>

