            <controller_action_predispatch_checkout_cart_add>
                <observers>
                    <smvendors>
                        <type>singleton</type>
                        <class>smvendors/observer</class>
                        <method>hookCheckoutCartAddPreDispatch</method>
                    </smvendors>
                </observers>
            </controller_action_predispatch_checkout_cart_add>

            <catalog_product_collection_load_after>
                <observers>
                    <smvendors>
                        <type>singleton</type>
                        <class>smvendors/price_observer</class>
                        <method>addFinalPriceForCustomerGroup</method>
                    </smvendors>
                </observers>
            </catalog_product_collection_load_after>

			<!-- hook customer create new account  -->
            <customer_register_success>
                <observers>
                    <smvendors>
                        <type>singleton</type>
                        <class>smvendors/observer</class>
                        <method>hookCustomerRegisterSuccess</method>
                    </smvendors>
                </observers>
            </customer_register_success>

            <controller_action_predispatch_customer_account_editPost>
                <observers>
                    <smvendors>
                        <type>singleton</type>
                        <class>smvendors/observer</class>
                        <method>hookCustomerAccountEditPostPreDispatch</method>
                    </smvendors>
                </observers>
            </controller_action_predispatch_customer_account_editPost>

            <sales_order_status_history_save_before>
                <observers>
                    <smvendors>
                        <type>singleton</type>
                        <class>smvendors/observer</class>
                        <method>hookSalesOrderStatusHistorySaveBefore</method>
                    </smvendors>
                </observers>
            </sales_order_status_history_save_before>

            <sales_order_place_after>
                <observers>
                    <smvendors>
                        <type>singleton</type>
                        <class>smvendors/observer</class>
                        <method>hookOrderPlaceAfter</method>
                    </smvendors>
                </observers>              
            </sales_order_place_after>		
		
        </events>

        <fieldsets>
            <sales_convert_quote_item>
                <vendor_id>
                    <to_order_item>*</to_order_item>
                </vendor_id>
            </sales_convert_quote_item>
        </fieldsets>

        <template>
            <email>
                <smvendors_email_order_template_new translate="label" module="smvendors">
                    <label>For Vendor: New Order</label>
                    <file>smvendors/order_new.html</file>
                    <type>html</type>
                </smvendors_email_order_template_new>
                <smvendors_email_order_template_new_to_vendor translate="label" module="smvendors">
                    <label>For Vendor: New Order Send to Vendor</label>
                    <file>smvendors/order_new_to_vendor.html</file>
                    <type>html</type>
                </smvendors_email_order_template_new_to_vendor>
                <smvendors_email_order_template_update translate="label" module="smvendors">
                    <label>For Vendor: Update Order</label>
                    <file>smvendors/order_update.html</file>
                    <type>html</type>
                </smvendors_email_order_template_update>
                <smvendors_email_invoice_template_new translate="label" module="smvendors">
                    <label>For Vendor: New Invoice</label>
                    <file>smvendors/invoice_new.html</file>
                    <type>html</type>
                </smvendors_email_invoice_template_new>
                <smvendors_email_invoice_template_update translate="label" module="smvendors">
                    <label>For Vendor: Update Invoice</label>
                    <file>smvendors/invoice_update.html</file>
                    <type>html</type>
                </smvendors_email_invoice_template_update>
                <smvendors_email_shipment_template_new translate="label" module="smvendors">
                    <label>For Vendor: New Shipment</label>
                    <file>smvendors/shipment_new.html</file>
                    <type>html</type>
                </smvendors_email_shipment_template_new>
                <smvendors_email_shipment_template_update translate="label" module="smvendors">
                    <label>For Vendor: Update Shipment</label>
                    <file>smvendors/shipment_update.html</file>
                    <type>html</type>
                </smvendors_email_shipment_template_update>
                <smvendors_email_creditmemo_template_new translate="label" module="smvendors">
                    <label>For Vendor: New Creditmemo</label>
                    <file>smvendors/creditmemo_new.html</file>
                    <type>html</type>
                </smvendors_email_creditmemo_template_new>
                <smvendors_email_creditmemo_template_update translate="label" module="smvendors">
                    <label>For Vendor: Update Creditmemo</label>
                    <file>smvendors/creditmemo_update.html</file>
                    <type>html</type>
                </smvendors_email_creditmemo_template_update>
                <smvendors_email_contact_vendor_template translate="label" module="smvendors">
                    <label>For Vendor: Contact Vendor</label>
                    <file>smvendors/contact_vendor.html</file>
                    <type>html</type>
                </smvendors_email_contact_vendor_template>

				<!-- Vendor create account email templates -->
                <customer_create_account_vendor_email_template translate="label" module="customer">
                    <label>New Vendor account</label>
                    <file>smvendors/vendor_account_new.html</file>
                    <type>html</type>
                </customer_create_account_vendor_email_template>
                <customer_create_account_vendor_email_confirmation_template translate="label" module="customer">
                    <label>New Vendor account confirmation key</label>
                    <file>smvendors/vendor_account_new_confirmation.html</file>
                    <type>html</type>
                </customer_create_account_vendor_email_confirmation_template>
                <customer_create_account_vendor_email_confirmed_template translate="label" module="customer">
                    <label>New Vendor account confirmed</label>
                    <file>smvendors/vendor_account_new_confirmed.html</file>
                    <type>html</type>
                </customer_create_account_vendor_email_confirmed_template>
                <customer_create_account_vendor_email_approved_template translate="label" module="customer">
                    <label>For Vendor: New Vendor Approved</label>
